{
  "expo": {
    "name": "restaurant",
    "slug": "restaurant",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/delivery.png",
    "splash": {
      "image": "./assets/delivery.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["CAMERA", "CAMERA_ROLL"],

      "adaptiveIcon": {
        "foregroundImage": "./assets/delivery.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yilmazito.restaurant"
    },
    "web": {
      "favicon": "./assets/delivery.png"
    }
  }
}
